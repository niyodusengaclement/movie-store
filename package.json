{
  "name": "oaf-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "test": "nyc --reporter=html --reporter=text mocha tests",
    "task-kill": "Taskkill /IM node.exe /F",
    "dev": "nodemon app.js",
    "coverage": "nyc --reporter=html --reporter=text mocha tests"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "hbs": "^4.1.1",
    "morgan": "~1.9.1",
    "node-pre-gyp": "^0.14.0",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "imgur": "^0.3.1",
    "mocha": "^7.1.2",
    "mocha-eslint": "^6.0.0",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "puppeteer": "^2.1.1"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "rules": {
      "semi": "warn",
      "eqeqeq": "warn",
      "curly": "warn",
      "no-bitwise": "warn"
    },
    "parserOptions": {
      "ecmaVersion": 8
    },
    "env": {
      "es6": true,
      "node": true
    }
  }
}
